<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="customer">
	<select id="select" resultType="customervo" parameterType="MAP">
		<![CDATA[
	select * from customer where id=#{id};
	]]>
	</select>
	<insert id="insert" parameterType="customervo">
		<![CDATA[
	insert into customer values (#{customer_code},#{name},#{contact},#{manager_name},#{manager_contact},#{manager_email},#{manager_birth},#{time},#{address},#{id},#{positionX},#{positionY});	
		]]>
	</insert>
	<update id="update" parameterType="customervo">
		<![CDATA[
		
		]]>
	</update>
	<select id="getPositionById" resultType="positionvo" parameterType="positionvo">
	<![CDATA[
	select name,positionX, positionY from customer where id=#{id};
	]]>
	</select>
</mapper>
